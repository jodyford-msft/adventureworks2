@page  
@model AdventureWorksModel  

<h1>AdventureWorks Data</h1>  

<table>  
    <thead>  
        <tr>  
            <th>First Name</th>  
            <th>Last Name</th>  
        </tr>  
    </thead>  
    <tbody>  
        @foreach (var person in Model.People)  
        {  
            <tr>  
                <td>@person.FirstName</td>  
                <td>@person.LastName</td>  
            </tr>  
        }  
    </tbody>  
</table>  
